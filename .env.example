# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/financial_data

# API Keys
ALPHA_VANTAGE_API_KEY=your_api_key_here

# Pipeline Configuration
LOG_LEVEL=INFO
BATCH_SIZE=100

# Ollama Configuration (for RAG demo)
# Point to your Ollama server (local or via Tailscale)
OLLAMA_HOST=http://localhost:11434
# Example Tailscale: http://100.x.x.x:11434

# RAG Configuration
RAG_LLM_MODEL=llama3.1:8b
RAG_EMBEDDING_MODEL=nomic-embed-text
RAG_TOP_K_RESULTS=3
